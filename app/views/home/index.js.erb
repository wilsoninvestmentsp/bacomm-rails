<% if @events.present? %>
  $('#event-listing').html("<%= escape_javascript(render partial: 'events', locals: { events: @events } ) %>");
  <% if @next_page.present? %>
    $('#next_page').attr('value', "<%= @next_page %>");
    $('#event_status').val("<%= params['events'] %>")
  <% else %>
    $('.load_more_events').hide();
  <% end %>
<% else %>
  $('.load_more_events').hide();
<% end %>